{
    "name": "adkats/bfacp",
    "description": "A web based admin tool for AdKats, a plugin for procon.",
    "keywords": ["adkats", "bfacp", "procon", "web admin"],
    "license": "proprietary",
    "homepage": "https://github.com/AdKats/BFAdminCP",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "guzzlehttp/guzzle": "~6.0",
        "lavary/laravel-menu": "^1.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "BFACP\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
